config firewall service category
    edit "BUBE_MGMT"
    next
    edit "BUBE_BIG_PLAYER"
    next
    edit "BUBE_VIDEO"
    next
    edit "BUBE_SPEEDTEST"
    next
    edit "BUBE_BACKUP"
    next
    edit "BUBE_MEDICAL"
    next
    edit "BUBE_VOIP"
    next
    edit "BUBE_MULTIMEDIA"
    next
    edit "BUBE_SIX_PAYMENT_SERVICES"
    next
    edit "BUBE_REMOTE"
    next
    edit "BUBE_KUNDE"
        set comment "Kundenspeziefische Ports und Services"
    next
    edit "BUBE_ANTIVIRUS"
        set comment "Spezifische Ports und Services fuer Antivirus Loesungen"
    next
    edit "BUBE_GAMES"
        set comment "Spezifische Ports und Services fuer Games"
    next
end

