config firewall service group
    edit "G_INTERNET_BUBE_V1.4"
        set member "DNS" "FTP" "HTTP" "HTTPS" "NTP" "HTTPS_UDP" "HTTP_TCP_8080" "HTTP_UDP_8080" "HTTP_UDP"
    next
    edit "G_MAIL_BUBE_V1.0"
        set member "IMAP" "IMAPS" "POP3" "POP3S" "SMTP" "SMTPS" "SMTP_TCP_587" "DNS"
    next
    edit "G_DIAGNOSE_BUBE_V1.4"
        set member "ALL_ICMP" "HTTPS_TCP_9443" "HTTPS_TCP_10443" "HTTPS_TCP_50443" "HTTPS_TCP_4343" "FORTINET-FORTICLIENT_TCP_8013" "CHECKMK_AGENT_TCP_6556" "SynologyFileStation"
    next
